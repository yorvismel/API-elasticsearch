    "/{indexName}/search": {
      "get": {
        "description": "Endpoint for searching and querying data in the specified Elasticsearch index.",
        "parameters": [
          {
            "name": "indexName",
            "in": "path",
            "required": true,
            "type": "string"
          },
          {
            "name": "q",
            "in": "query",
            "type": "string",
